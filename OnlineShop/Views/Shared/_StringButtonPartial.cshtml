@*@Model int// id show in index*@
@model string
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<td style="width:120px">
    <div class="btn-group">
        <a asp-action="Edit" class="btn btn-primary" asp-route-id="@Model">
            <i class="fas fa-file-edit"></i>
        </a>
        <a asp-action="Details" class="btn btn-success" asp-route-id="@Model">
            <i class="far fa-list-alt"></i>
        </a>
        @*<a asp-action="Delete" asp-controller="SpecialTag" class="btn btn-danger" asp-route-id="@Model">*@
        <a asp-action="Lockout" class="btn btn-danger" asp-route-id="@Model">

            <i class="fa fa-ban"></i>
        </a>
    </div>
</td>
